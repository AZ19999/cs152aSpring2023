<div class="data-form">
  <form action="/users/create" method="POST">
    <h2>Create a new event:</h2>
    <label for="inputTitle">Title</label>
    <input
      type="text"
      name="title"
      id="inputTitle"
      placeholder="New Event"
      required
      autofocus
    />
    <label for="inputDescription">Description</label>
    <input
      type="text"
      name="description"
      id="inputDescription"
      placeholder="A Cool Event"
      required
    />
    <label for="inputLocation">Location</label>
    <input
      type="text"
      name="location"
      id="inputLocation"
      placeholder="Office"
      required
    />
    <label for="inputStartDate">Start Date</label>
    <input
      type="text"
      name="startDate"
      id="inputStartDate"
      placeholder="tomorrow"
      required
    />
    <label for="inputEndDate">End Date</label>
    <input
      type="text"
      name="endDate"
      id="inputEndDate"
      placeholder="tomorrow"
      required
    />
    <label for="inputIsOnline">Is it Online?</label>
    <input
      type="Boolean"
      name="isOnline"
      id="inputIsOnline"
      required
    />
    <label for="inputRegistrationLink">Registration Link</label>
    <input
      type="text"
      name="registrationLink"
      id="inputRegistrationLink"
      placeholder="URL"
    />
    <label for="inputOrganizer">Organizer</label>
    <input
      type="text"
      name="Organizer"
      id="inputOrganizer"
      placeholder="Organizer"
      required
    />
    <label for="inputAttendees">Attendees</label>
    <input
      type="text"
      name="Attendees"
      id="inputAttendees"
      placeholder="5"
    />
    <button type="submit">Create</button>
  </form>
</div>
