<h2>Events Table</h2>
<p>
  We love hosting events, if you have any questions don't hesitate to ask us!
</p>
<table class="table">
  <thead>
    <tr>
      <th>Title</th>
      <th>Location</th>
      <th>Start Date</th>
    </tr>
  </thead>
  <tbody>
    <% events.forEach(event => { %>
    <tr>
      <td><a href="<%= `/events/${event._id}`%>"><%= event.Title %></a></td>
      <td><%= event.location %></td>
      <td><%= event.startDate %></td>
      <td>
        <a href="<%=`/events/${event._id}/edit` %>"> Edit </a>
      </td>
      <td>
        <a
          href="<%= `events/${event._id}/delete?_method=DELETE` %>"
          onclick="return confirm('Are you sure you want to delete this record?')"
          >Delete</a
        >
      </td>
    </tr>
    <% }); %>
  </tbody>
</table>
